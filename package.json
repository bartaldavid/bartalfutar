{
  "name": "bartalfutar-sveltekit",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "generate": "drizzle-kit generate:mysql",
    "db:push": "drizzle-kit push:mysql",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --write  --plugin prettier-plugin-svelte .",
    "generate:schema": "openapi-typescript https://opendata.bkk.hu/docs/futar-openapi.yaml -o ./src/lib/schema-generated.ts"
  },
  "devDependencies": {
    "@iconify-json/material-symbols": "^1.1.82",
