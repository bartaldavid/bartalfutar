  "version": "0.5.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "generate": "drizzle-kit generate:mysql",
    "db:push": "drizzle-kit push:mysql",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
