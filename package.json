    "svelte": "5.0.0-next.151",
    "svelte-check": "^3.8.0",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.6.3",
    "typescript": "^5.4.5",
    "unplugin-icons": "^0.19.0",
    "vite": "^5.2.13"
